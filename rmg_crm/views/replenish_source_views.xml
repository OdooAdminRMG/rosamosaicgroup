<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="replenish_sources_tree" model="ir.ui.view">
            <field name="name">replenish.sources.tree</field>
            <field name="model">replenish.sources</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="po_id" invisible="True"/>
                    <field name="so_line_id" invisible="True"/>
                    <field name="mo_origin" invisible="True"/>
                    <field name="mo_id" invisible="True"/>
                    <field name="so_id" invisible="True"/>
                    <field name="job_name"/>
                    <field name="product_id"/>
                    <field name="product_uom_qty" invisible="True"/>
                    <field name="price_unit"/>
                    <field name="requested_by"/>
                    <field name="requested_on"/>
                </tree>
            </field>
        </record>
    </data>
</odoo>
