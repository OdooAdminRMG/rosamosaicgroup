<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="product_template_only_form_view_inherit_rmg_mrp" model="ir.ui.view">
        <field name="name">product.template.product.form.rmg_mrp</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="product.product_template_only_form_view"/>
        <field name="arch" type="xml">
            <field name="tracking" position="after">
                <field name="check_relieve_full_lot"/>
            </field>
        </field>
    </record>
    <record id="product_normal_form_view_inherit_rmg_mrp" model="ir.ui.view">
        <field name="name">product.product.form.rmg_mrp</field>
        <field name="model">product.product</field>
        <field name="inherit_id" ref="product.product_normal_form_view"/>
        <field name="arch" type="xml">
            <field name="tracking" position="after">
                <field name="check_relieve_full_lot"/>
            </field>
        </field>
    </record>
</odoo>
